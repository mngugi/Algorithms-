   "source": [
    "- Step 1: Input: A number n\n",
    "- Step 2: If n is perfectly divisible by 2, \n",
    "         - Then print \"The number is even\" \n",
    "- Step 3: Else\n",
    "         - Print \"The number is odd\"\n",
    "- Step 4: End"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "f1b45059-e26d-469f-857b-462ea16378f2",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The number 5 is odd\n"
     ]
    }
   ],
   "source": [
    "n = 5\n",
    "\n",
    "if n % 2 == 0:\n",
    "    print(f\"The number {n} is even\")\n",
    "else:\n",
    "    print(f\"The number {n} is odd\")"
   ]
  },
